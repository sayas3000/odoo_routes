<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record model="ir.ui.view" id="partner_instructor_form_view">
        <field name="name">list.client.routes</field>
            <field name="model">res.partner</field>
            <!--esto evita errores-->
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <notebook>
                    <page string="Rutas">
                        <group string="Rutas" name="routes" modifiers="{}"/>
                        <field name="list_routes" string="Lista de Rutas">
                                <tree editable ="bottom">
                                    <!--field name="sequence" widget="handle"/-->
                                    <field name="client_id" can_create="true" can_write="true"></field>
                                    <field name="orders" can_create="true" can_write="true"></field>
                                    <field name="route" can_ceate="true" can_write="true"></field>
                                </tree>
                        </field>
                        <group>
                            <button type="action" class="btn-link" name="odoo_routes.action_view_routes" string = "Ver detalle de rutas"/>
                        </group>
                    </page>
                </notebook>
            </field>
        </record>
</odoo>